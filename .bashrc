# .bashrc

# cmd aliases
alias ll="ls -l"
alias la="ls -a"
alias lh="ls -h"
alias rmr="rm -r"
alias l="ls"

# git aliases
alias gs="git status"
alias gd="git diff"
alias gco="git checkout"
alias gb="git branch"
alias ga="git add -p"
alias gcm="git commit -m"
alias gps="git push"
alias gpl="git pull"

# editor aliases
alias e="emacs --no-splash &"

